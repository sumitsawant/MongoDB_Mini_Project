<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>College Administration System</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>studentServ</servlet-name>
    <servlet-class>com.servlets.StudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>studentServ</servlet-name>
    <url-pattern>/StudentServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>staffServ</servlet-name>
    <servlet-class>com.servlets.StaffServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>staffServ</servlet-name>
    <url-pattern>/StaffServlet</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>LoginServ</servlet-name>
    <servlet-class>com.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServ</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
</web-app>
